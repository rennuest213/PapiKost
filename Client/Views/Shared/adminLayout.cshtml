    <script>
        $(document).ready(function () {
            console.log(sessionStorage.getItem("token"))
            $('#tableKost').DataTable({
                ajax: {
                    url: 'https://localhost:7073/api/Kost',
                    type: 'GET',
                    dataSrc: 'data',
                    headers: {
                        'Authorization': "Bearer " + sessionStorage.getItem("token"),
                    },
                },
                columns: [
                    {
                        data: 'id',
                    },
                    {
                        data: 'name',
                    },
                    {
                        data: 'harga',
                    },
                    {
                        data: 'jenis',
                    },
                    {
                        data: 'alamat',
                    },
                    {
                        data: null,
                        "render": function (data, type, row, meta) {
                            return `<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#detailsKost" onclick="detailsKost('${data.id}')">Details</button>
                                                            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#editKost" onclick="editKost('${data.id}')">Edit</button>
                                                            <button type="button" class="btn btn-danger" onclick="deleteKost('${data.id}')">Delete</button>`;
                        }
                    }

                ],
                dom: 'Bfrtip',
                buttons: [
                    'copyHtml5',
                    'excelHtml5',
                    'csvHtml5',
                    'pdfHtml5',
                    'colvis'
                ],

            });
        });
        function create() {
            var Name = $('#name').val();
            var Harga = parseInt($('#harga').val());
            var Jenis = $('#jenis').val();
            var Alamat = $('#alamat').val();
            var Spesifikasi = $('#spesifikasi').val();
            var Keterangan = $('#keterangan').val();
            var Image = $('#image').val();
            var PemilikID =  $('#pemilik').val();

            var res = { Name, Harga, Jenis, Alamat, Spesifikasi, Keterangan, Image, PemilikID };
                $.ajax({
                    url: `https://localhost:7073/api/Kost`,
                    type: "POST",
                    headers: {
                        'Authorization': "Bearer " + sessionStorage.getItem("token"),
                    },
                    contentType: "application/json; charset=utf-8", 
                    dataType: "json",
                    data: JSON.stringify(res),
                    success: function () {
                        Swal.fire("Success", "Success To Create.", "success").then(function () {
                            location.reload()
                        })
                    },
                    error: function () {
                    }
                });
        }

        function detailsKost(id) {
            $.ajax({
                url: `https://localhost:7073/api/Kost/${id}`,
                //dataSrc: 'data',
                headers: {
                    'Authorization': "Bearer " + sessionStorage.getItem("token"),
                },
                
                type: "GET",
            }).done((res) => {
                let temp = "";
                temp += `
                                                <input type="hidden" class="form-control" id="hideId" readonly placeholder="" value="0">
                                                <p>Id: </p><input type="text" class="form-control" id="kostId" readonly placeholder="${res.data.id}" value="${res.data.id}">
                                                <p>Name: </p><input type="text" class="form-control" id="kostName" readonly placeholder="${res.data.name}" value="${res.data.name}">
                                                <p>Harga: </p><input type="text" class="form-control" id="hargaKost" readonly placeholder="${res.data.harga}" value="${res.data.harga}">
                                                <p>Jenis: </p><input type="text" class="form-control" id="jenisKost" readonly placeholder="${res.data.jenis}" value="${res.data.jenis}">
                                                <p>Alamat: </p><input type="text" class="form-control" id="alamatKost" readonly placeholder="${res.data.alamat}" value="${res.data.alamat}">
                                                <p>Spesifikasi: </p><input type="text" class="form-control" id="spesifikasiKost" readonly placeholder="${res.data.spesifikasi}" value="${res.data.spesifikasi}">
                                                <p>Keterangan: </p><input type="text" class="form-control" id="keteranganKost" readonly placeholder="${res.data.keterangan}" value="${res.data.keterangan}">
                                                <p>Image: </p><img src="${res.data.image}" alt="Kost Bob">
                                                <p>PemilikID: </p><input type="text" class="form-control" id="pemilikID" readonly placeholder="${res.data.pemilikID}" value="${res.data.pemilikID}">
                                                `;
                $("#tampilDetails").html(temp);
                console.log(res);
            }).fail((err) => {
                console.log(err);
            });
        }
        function editKost(id) {
            $.ajax({
                url: `https://localhost:7073/api/Kost/${id}`,
                headers: {
                    'Authorization': "Bearer " + sessionStorage.getItem("token"),
                },
                type: "GET"
            }).done((res) => {
                let temp = "";
                temp += `
                                                <input type="hidden" class="form-control" id="hideId" readonly placeholder="" value="0">
                                                <p>Id: </p><input type="text" class="form-control" id="kostId" readonly placeholder="${res.data.id}" value="${res.data.id}">
                                                <p>Name: </p><input type="text" class="form-control" id="kostName" placeholder="${res.data.name}" value="${res.data.name}">
                                                <p>Harga: </p><input type="text" class="form-control" id="hargaKost" placeholder="${res.data.harga}" value="${res.data.harga}">
                                                <p>Jenis: </p><input type="text" class="form-control" id="jenisKost" placeholder="${res.data.jenis}" value="${res.data.jenis}">
                                                <p>Alamat: </p><input type="text" class="form-control" id="alamatKost" placeholder="${res.data.alamat}" value="${res.data.alamat}">
                                                <p>Spesifikasi: </p><input type="text" class="form-control" id="spesifikasiKost" placeholder="${res.data.spesifikasi}" value="${res.data.spesifikasi}">
                                                <p>Keterangan: </p><input type="text" class="form-control" id="keteranganKost" placeholder="${res.data.keterangan}" value="${res.data.keterangan}">
                                                <p>Image: </p><input type="text" class="form-control" id="imageKost" placeholder="${res.data.image}" value="${res.data.image}">
                                                <p>PemilikID: </p><input type="text" class="form-control" id="pemilikID" placeholder="${res.data.pemilikID}" value="${res.data.pemilikID}">
                                                <p></p>
                                                <button type="button" class="btn btn-primary" id="editBtn" onclick="saveEdit('${res.data.id}')">Save changes</button>
                                                `;
                $("#tampilEdit").html(temp);
            }).fail((err) => {
                console.log(err);
            });
        }

        function saveEdit(id) {
            //console.log(parseInt($('#departId').val()));
            //var disini harus sesuai sama nama table
            var Id = id;
            var Name = $('#kostName').val();
            var Harga = parseInt($('#hargaKost').val());
            var Jenis = $('#jenisKost').val();
            var Alamat = $('#alamatKost').val();
            var Spesifikasi = $('#spesifikasiKost').val();
            var Keterangan = $('#keteranganKost').val();
            var Image = $('#imageKost').val();
            var PemilikID = $('#pemilikID').val();


            //console.log(Id);
            //console.log(Name);
            //console.log(divId);

            var res = { Id, Name, Harga, Jenis, Alamat, Spesifikasi, Keterangan, Image, PemilikID };
            Swal.fire({
                title: 'Do you want to save the changes?',
                showDenyButton: true,
                showCancelButton: true,
                confirmButtonText: 'Save',
                denyButtonText: `Don't save`,
            }).then((result) => {
                /* Read more about isConfirmed, isDenied below */
                if (result.isConfirmed) {
                    $.ajax({
                        url: `https://localhost:7073/api/Kost`,
                        type: "PUT",
                        headers: {
                            'Authorization': "Bearer " + sessionStorage.getItem("token"),
                        },
                        contentType: "application/json",
                        dataType: "json",
                        data: JSON.stringify(res),
                        success: function () {
                            Swal.fire("Saved!", "Your File Has Been Updated", "success").then(function () {
                                location.reload()
                            })
                            location.reload();
                        },
                        error: function (xhr, ajaxOptions, throwError) {
                            Swal.fire("Error Updated!", "Please Try Again", "error");
                        }
                    });
                }
                    //Swal.fire('Saved!', '', 'success')
                else if (result.isDenied) {
                    Swal.fire('Changes are not saved', '', 'info')
                }
            })
        }

        function deleteKost(id) {
            Swal.fire({
                title: 'Are you sure?',
                text: "You won't be able to revert this!",
                type: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, delete it!'
            }).then((result) => {
                if (result.isConfirmed) {
                    $.ajax({
                        url: `https://localhost:7073/api/Kost/${id}`,
                        type: "DELETE",
                        headers: {
                            'Authorization': "Bearer " + sessionStorage.getItem("token"),
                        },
                        datatype: 'json',
                        success: function () {
                            Swal.fire("Deleted", "Your file has been deleted.", "success").then(function () {
                                location.reload()
                            })
                        },
                        error: function (xhr, ajaxOptions, throwError) {
                            Swal.fire("Error deleting!", "Please Try Again", "error");
                        }
                    });
                }
            });
        }
       
    </script>
